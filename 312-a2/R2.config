interfaces {
    ethernet eth1 {
        address 10.0.54.82/30
        description "The link between R2 and R1"
        duplex auto
        hw-id 00:aa:00:80:01:01
        smp_affinity auto
        speed auto
    }
    ethernet eth2 {
        address 10.0.54.78/30
        description "The link between R2 and R0"
        duplex auto
        hw-id 00:aa:00:80:00:01
        smp_affinity auto
        speed auto
    }
    loopback lo {
        address 10.0.54.67/32
    }
}
protocols {
    ospf {
        area 0.0.0.0 {
            network 10.0.54.76/30
            network 10.0.54.80/30
        }
        parameters {
            abr-type cisco
            router-id 10.0.54.67
        }
        redistribute {
            connected {
                metric-type 2
            }
        }
    }
}
system {
    host-name R2
    login {
        user vyatta {
            authentication {
                encrypted-password $1$1WtdCQ3x$tEayWu2oe8dwau6SquaBe0
            }
            level admin
        }
    }
    ntp-server 0.vyatta.pool.ntp.org
    package {
        auto-sync 1
        repository supported {
            components main
            distribution stable
            password ""
            url http://packages.vyatta.com/vyatta-supported
            username ""
        }
    }
    syslog {
        global {
            facility all {
                level notice
            }
            facility protocols {
                level debug
            }
        }
    }
    time-zone GMT
}


